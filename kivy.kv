<HomePage>
    name: 'home'
    canvas.before:
        Rectangle:
            size:self.size
            source: '1155379.jpg'


    Button:
        id:con
        text:'Continue'
        background_color: 0,0,0,0

        size_hint: 0.2,0.1
        pos_hint: {"x":0.37,"y":0.10}

        text_size: self.width,None
        size_hint: 1.5, 0.1
        height: self.texture_size[1]
        font_size: self.width/23
        color: 0.8,0.7,1,1 # light purple

        on_release:app.root.current = 'q1'
        #on_release:root.decide()



<Q1>
    name: 'q1'
    BoxLayout:
        orientation: 'vertical'
        canvas.before:
            Rectangle:
                size:self.size
                source: 'q1.jpg'

        Label:

            text:'\n     First of all, in which age group\n                 do you belong in?\n'
            text_size: self.width,None
            size_hint: 1,1.3
            height: self.texture_size[1]
            font_size: self.width/15
            color: 0,0,0

            canvas.before:
                Rectangle:
                    pos: self.pos
                    size:self.size

        Button:

            text: 'a. 1-2 years old'
            text_size: self.width,None
            halign: 'center'
            font_size: self.width/18

            background_color: 0,0,0,0
            color: (0,0,0)
            on_press:root.pressbtn('a','a','b','c','d','e')
            on_release:app.root.current = 'q3'



        Button:

            text: 'b. 3-5 years old'
            text_size: self.width,None
            halign: 'center'
            font_size: self.width/18

            background_color: 0,0,0,0
            color: (1,0,0)
            on_press:root.pressbtn('b','a','b','c','d','e')
            on_release:app.root.current = 'q3'

        Button:

            text: 'c. 6-12 years old'
            text_size: self.width,None
            halign: 'center'
            font_size: self.width/18

            background_color: 0,0,0,0
            color: (1,0,0)
            on_press:root.pressbtn('c','a','b','c','d','e')
            on_release:app.root.current = 'q3'

        Button:

            text: 'd. 13-18 years old'
            text_size: self.width,None
            halign: 'center'
            font_size: self.width/18

            background_color: 0,0,0,0
            color: (1,0,0)
            on_press:root.pressbtn('d','a','b','c','d','e')
            on_release:app.root.current = 'q3'

        Button:

            text: 'e. 18+ years old'
