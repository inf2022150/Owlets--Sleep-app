<Q4>

    name: 'q4'
    BoxLayout:
        orientation: 'vertical'

        canvas.before:
            Rectangle:
                size:self.size
                source: '1155379.jpg'

        Label:

            text:'\n \n             Lastly, what will your\n                    username be?'
            size_hint_y:0.7

            text_size: self.width,None
            height: self.texture_size[1]
            font_size: self.width/15
            color: 0,0,0

        Label:

            size_hint_y:2.4

        Label:

            size_hint_y:0.3
            text: 'Enter Username:'
            font_size: self.width/21

        BoxLayout:
            AnchorLayout:
                TextInput:
                    id: user_input
                    multiline:False
                    pos_y:1.2
                    size_hint: 0.6,0.5
                    font_size:self.width/13

        BoxLayout:

            Button:

                id: register
                size_y:0.5
                text:'Submit'

                font_size: self.width/21

                on_release:root.register()
                on_release:app.root.current = 'comments'

        Label:

            size_hint_y:3.2



<Comments>

